{
  "name": "medium-draft",
  "version": "0.3.2",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/static/ && rm -rf lib",
    "dev": "mkdir -p dist && cp index.html dist/index.html && NODE_ENV=development webpack-dev-server --host 0.0.0.0 --devtool eval --content-base dist/ --progress --colors --hot",
    "build": "npm run clean && NODE_ENV=production webpack",
    "babel": "NODE_ENV=production babel ./src --out-dir lib --copy-files --ignore 'script.js'",
    "npm": "npm run build && ./node_modules/.bin/babel ./src -d lib --ignore 'example.js' && mv dist/static/example.css dist/static/medium-draft.css && cp dist/static/medium-draft.css ./lib/index.css"
  },
  "keywords": [],
  "author": "Brijesh Bittu <brijeshb42@gmail.com> (http://bitwiser.in/)",
  "license": "MIT",
  "dependencies": {
    "draft-js": "^0.8.0",
    "immutable": "^3.8.1",
    "react-addons-css-transition-group": "^15.2.1"
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-plugin-babel": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.2.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "bugs": {
    "url": "https://github.com/brijeshb42/medium-draft/issues"
  },
  "homepage": "http://bitwiser.in/medium-draft/",
